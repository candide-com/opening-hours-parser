{
  "compilerOptions": {
    "typeRoots": ["./typings", "./node_modules/@types"],
    "outDir": "./dist",
    "target": "es2017",
    "lib": ["es2017"],
    "module": "commonjs",
    "noUnusedLocals": true,
    "noErrorTruncation": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
